            android:entryValues="@array/pref_sync_values"/>
    </de.gasthof_schnau.gasthofschnau.lib.MyPreferenceCategory>

    <de.gasthof_schnau.gasthofschnau.lib.MyPreferenceCategory
        android:title="Speisekarte"
        android:key="pref_key_speisekarte">
        <de.gasthof_schnau.gasthofschnau.lib.MyListPreference
            android:title="Daten Aktualisieren"
            android:key="pref_key_sync_speisekarte"
            android:defaultValue="Automatisch"
